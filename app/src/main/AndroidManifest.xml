<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaoniu.animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".transition.VectorAnimationActivity"></activity>
        <activity android:name=".transition.ShareDetailActivity" />
        <activity android:name=".transition.ShareListActivity" />
        <activity android:name=".transition.ShareActivity" />
        <activity android:name=".transition.FadeActivity" />
        <activity android:name=".transition.SlideActivity" />
        <activity android:name=".transition.ExplodeActivity" />
        <activity android:name=".transition.OldTransitionActivity" />
        <activity
            android:name=".transition.TransitionAnimationActivity"
            android:label="@string/transition_animation" />
        <activity
            android:name=".reveal.RevealAnimationActivity"
            android:label="@string/animation_reveal" />
        <activity
            android:name=".frame.FrameAnimationActivity"
            android:label="@string/animation_frame" />
        <activity
            android:name=".property.PropertyAnimationActivity"
            android:label="@string/animation_property" />
        <activity
            android:name=".tween.TweenAnimationActivity"
            android:label="@string/animation_tween" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>